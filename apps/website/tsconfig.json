{
  "exclude": ["node_modules"],
  "extends": "@codingam/tsconfig/nextjs.json",
  "compilerOptions": {
    "outDir": "dist",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": ["src", "next-env.d.ts", ".next/types/**/*.ts"],
    "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
